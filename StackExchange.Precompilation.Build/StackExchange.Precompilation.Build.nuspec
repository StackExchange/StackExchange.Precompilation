<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>StackExchange.Precompilation.Build</id>
    <version>$version$</version>
    <authors>Stack Exchange inc.</authors>
    <owners>m0sa</owners>
    <projectUrl>https://github.com/StackExchange/StackExchange.Precompilation</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Replaces CSC and aspnet_compiler.exe with StackExchange.Precompiler for compiling C# (.cs) and Razor View (.cshtml) files in asp.net mvc 5 projects.</description>
    <summary>StackExchange Precompilation Build Package</summary>
    <copyright>Stack Exchange 2015</copyright>
    <language>en-US</language>
    <tags>Razor AspNet MsBuild Roslyn Metaprogramming</tags>
    <dependencies>
      <dependency id="StackExchange.Precompilation.Metaprogramming" version="$version$" />
    </dependencies>
  </metadata>
  <files>
      <file src="bin\Release\StackExchange.Precompiler.*" target="tools" />
      <file src="*.targets" target="build" /><!-- .targets file name must match the package id -->
  </files>
</package>